{
  "name": "cta-io",
  "version": "1.0.0",
  "description": "input/output to cta components",
  "author": "CTA Team <cta@thomsonreuters.com>",
  "main": "lib/index.js",
  "scripts": {
    "test": "NODE_PATH=. _mocha",
    "codecoverage": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "lint": "eslint --quiet lib test",
    "docs": "jsdoc --readme README.md lib -r -d docs && opener ./docs/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:thomsonreuters/cta-io.git"
  },
  "engines": {
    "node": "6.9.4"
  },
  "devDependencies": {
    "@thomsonreuters/eslint-config-cta": "^1.0.0",
    "chai": "~3.4.0",
    "chai-as-promised": "~5.1.0",
    "co-sleep": "0.0.1",
    "coveralls": "^3.0.0",
    "eslint": "^3.2.2",
    "istanbul": "~0.4.1",
    "jsdoc": "~3.4.0",
    "mocha": "~2.3.4",
    "moment": "~2.11.1",
    "node-schedule": "~0.6.0",
    "opener": "~1.4.0",
    "sinon": "~1.17.0"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "cta-flowcontrol": "git+ssh://git@github.com:thomsonreuters/cta-flowcontrol.git#1.0.0",
    "cta-common": "git+ssh://git@github.com:thomsonreuters/cta-common.git#1.0.0",
    "cta-brick": "git+ssh://git@github.com:thomsonreuters/cta-brick.git#1.0.0",
    "cta-messaging": "git+ssh://git@github.com:thomsonreuters/cta-messaging.git#1.0.0",
    "co": "~4.6.0",
    "lodash": "~4.11.0",
    "shortid": "~2.2.4"
  }
}
